<h1>Delegate added</h1>

<table class="edit">
  <tr>
    <td>Name</td>
    <td><a href="/delegates/<%= @delegate.id %>"><%= @delegate.name %></a></td>
  </tr>
  <tr>
    <td>Board member?</td>
    <td <%= @delegate.board_member=="yes" ? "class='green'" : "class='red'" %>><%= @delegate.board_member %></td>
  </tr>
</table>

<br>
