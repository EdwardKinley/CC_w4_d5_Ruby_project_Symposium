<h1>Add new presentation</h1>

<form method="post" action="/presentations">
  <table class="edit">
    <tr>
      <td>
        <label for="name">Name</label>
      </td>
      <td>
        <input type="text" name="name" id="name" required/>
      </td>
    </tr>
    <tr>
      <td>
        <label for="time_slot">Time slot</label>
      </td>
      <td>
        <input type="number" min="1" max="6" name="time_slot" id="time_slot" required/>
      </td>
    </tr>
    <tr>
      <td>
        <label for="capacity">Capacity</label>
      </td>
      <td>
        <input type="number" min="0" max="999" name="capacity" id="capacity" required/>
      </td>
    </tr>
      <td class="blank">
      </td>
      <td>
        <input type="submit" value="Submit"/>
      </td>
    </tr>
  </table>
</form>

<br>
